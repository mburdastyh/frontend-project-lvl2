{
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jest": true
    },
    "parserOptions": {
        "ecmaVersion": 10,
        "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "import/extensions": "off",
      "import/prefer-default-export": "off",
      "no-unused-vars": "off"
    },
    "extends": "airbnb-base"
}
